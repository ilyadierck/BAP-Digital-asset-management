s","original":{"line":9,"column":2}},{"generated":{"line":49,"column":0},"source":"node_modules/ipfs-http-client/src/dht/provide.js","original":{"line":43,"column":1}},{"generated":{"line":49,"column":1},"source":"node_modules/ipfs-http-client/src/dht/provide.js","original":{"line":8,"column":26}},{"generated":{"line":49,"column":2},"source":"node_modules/ipfs-http-client/src/dht/provide.js","original":{"line":8,"column":0}}],"sources":{"node_modules/ipfs-http-client/src/dht/provide.js":"'use strict'\n\nconst CID = require('cids')\nconst multiaddr = require('multiaddr')\nconst toCamel = require('../lib/object-to-camel')\nconst configure = require('../lib/configure')\n\nmodule.exports = configure(api => {\n  return async function * provide (cids, options = {}) {\n    cids = Array.isArray(cids) ? cids : [cids]\n\n    const searchParams = new URLSearchParams(options)\n    cids.forEach(cid => searchParams.append('arg', `${new CID(cid)}`))\n\n    const res = await api.ndjson('dht/provide', {\n      method: 'POST',\n      timeout: options.timeout,\n      signal: options.signal,\n      searchParams\n    })\n\n    for await (let message of res) {\n      // 3 = QueryError\n      // https://github.com/libp2p/go-libp2p-core/blob/6e566d10f4a5447317a66d64c7459954b969bdab/routing/query.go#L18\n      // https://github.com/ipfs/go-ipfs/blob/eb11f569b064b960d1aba4b5b8ca155a3bd2cb21/core/commands/dht.go#L283-L284\n      if (message.Type === 3) {\n        throw new Error(message.Extra)\n      }\n\n      message = toCamel(message)\n      message.id = new CID(message.id)\n      if (message.responses) {\n        message.responses = message.responses.map(({ ID, Addrs }) => ({\n          id: ID,\n          addrs: (Addrs || []).map(a => multiaddr(a))\n        }))\n      } else {\n        message.responses = []\n      }\n      yield message\n    }\n  }\n})\n"},"lineCount":null}},"error":null,"hash":"dffa55b46a15b6b5eac451c2327a4c14","cacheData":{"env":{}}}